<div class="container">
  <div class="row">
    <div class="col-8 p-5"></div>
      <h1>My List</h1>
      <% @lists.each do |list| %>
        <div class="card mb-3">
        <div class="card-body">
          <%= link_to list.name, list_path(list) %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
